<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
         version="3.1">
    <enterprise-beans>
        <entity>
            <display-name>Student</display-name>
            <ejb-name>StudentEJB</ejb-name>
            <home>com.berest.beans.student.StudentHome</home>
            <remote>com.berest.beans.student.StudentRemote</remote>
            <ejb-class>com.berest.beans.student.StudentBean</ejb-class>
            <persistence-type>Bean</persistence-type>
            <prim-key-class>java.lang.Integer</prim-key-class>
            <reentrant>true</reentrant>
        </entity>
        <entity>
            <display-name>Location</display-name>
            <ejb-name>LocationEJB</ejb-name>
            <home>com.berest.beans.location.LocationHome</home>
            <remote>com.berest.beans.location.LocationRemote</remote>
            <ejb-class>com.berest.beans.location.LocationBean</ejb-class>
            <persistence-type>Bean</persistence-type>
            <prim-key-class>java.lang.Integer</prim-key-class>
            <reentrant>true</reentrant>
        </entity>
        <entity>
            <ejb-name>CourseEJB</ejb-name>
            <home>com.berest.beans.course.CourseHome</home>
            <remote>com.berest.beans.course.CourseRemote</remote>
            <ejb-class>com.berest.beans.course.CourseBean</ejb-class>
            <persistence-type>Bean</persistence-type>
            <prim-key-class>java.lang.Integer</prim-key-class>
            <reentrant>true</reentrant>
        </entity>
        <entity>
            <ejb-name>GradeEJB</ejb-name>
            <home>com.berest.beans.grade.GradeHome</home>
            <remote>com.berest.beans.grade.GradeRemote</remote>
            <ejb-class>com.berest.beans.grade.GradeBean</ejb-class>
            <persistence-type>Bean</persistence-type>
            <prim-key-class>java.lang.Integer</prim-key-class>
            <reentrant>true</reentrant>
        </entity>
    </enterprise-beans>
</ejb-jar>
